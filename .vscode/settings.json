{
  "cSpell.words": [
    "clockadjust",
    "cmajflt",
    "cminflt",
    "cnswap",
    "cxxopts",
    "DEINITIALIZER",
    "GETCPU",
    "GETEVENT",
    "hypervisor",
    "INTENTER",
    "INTEXIT",
    "itrealvalue",
    "KERCALL",
    "KERCALLENTER",
    "KERCALLEXIT",
    "KERCALLINT",
    "kstkeip",
    "lpthread",
    "nproc",
    "qtime",
    "rsslim",
    "SETEVENT",
    "softirq",
    "startcode",
    "startstack",
    "SYSPAGE",
    "traceevent",
    "VTHREAD"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.formatOnSave": true,
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format",
    "editor.wordBasedSuggestions": "off",
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true
  },
  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "C_Cpp.codeAnalysis.clangTidy.useBuildPath": true,
  "C_Cpp.loggingLevel": "Debug",
  "C_Cpp.formatting": "clangFormat",
  "files.associations": {
    "cstdint": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "*.tcc": "cpp",
    "cctype": "cpp",
    "chrono": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "unordered_map": "cpp",
    "vector": "cpp",
    "exception": "cpp",
    "algorithm": "cpp",
    "functional": "cpp",
    "iterator": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "ratio": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "fstream": "cpp",
    "initializer_list": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "typeinfo": "cpp",
    "bitset": "cpp",
    "csignal": "cpp",
    "cstring": "cpp",
    "unordered_set": "cpp",
    "map": "cpp",
    "regex": "cpp",
    "cinttypes": "cpp",
    "condition_variable": "cpp",
    "mutex": "cpp",
    "thread": "cpp"
  },
  "editor.tabCompletion": "onlySnippets",
  "editor.snippetSuggestions": "inline",
  "emmet.showSuggestionsAsSnippets": true,
  "files.exclude": {
    "**/build": true,
    "**/log": true
  },
  "search.exclude": {
    "**/build": true,
    "**/log": true
  },
  "C_Cpp.default.defines": [
    "DEBUG"
  ],
  "C_Cpp.default.includePath": [
    "${workspaceFolder}/**/**",
    "${workspaceFolder}/**"
  ],
  "clang-tidy.blacklist": [
    "cxxopts.hpp"
  ],
  // "C_Cpp.codeAnalysis.clangTidy.config": ".clang-tidy",
  "C_Cpp.errorSquiggles": "disabled",
  "C_Cpp.codeAnalysis.runAutomatically": true,
  "C_Cpp.codeAnalysis.clangTidy.checks.disabled": [
    "bugprone-easily-swappable-parameters"
  ],
}